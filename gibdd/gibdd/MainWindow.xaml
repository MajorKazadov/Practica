<Window x:Class="gibdd.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:gibdd"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox Name="login" HorizontalAlignment="Left" Height="23" Margin="300,135,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox Name="password" HorizontalAlignment="Left" Height="23" Margin="300,194,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button Name="enterButton" Content="Enter" HorizontalAlignment="Left" Margin="316,273,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
        <Label Content="Login" HorizontalAlignment="Left" Margin="300,104,0,0" VerticalAlignment="Top"/>
        <Label Content="Password" HorizontalAlignment="Left" Margin="300,163,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.394,0.498"/>
        <Label Name="check" Content="Check" HorizontalAlignment="Left" Margin="513,163,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
